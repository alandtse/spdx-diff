{
  "name": "spdx-license-diff",
  "private": true,
  "version": "0.2.0",
  "description": "Diff selected text against SPDX licenses",
  "scripts": {
    "start": "yarn run dev:chrome",
    "build": "yarn run build:chrome",
    "build:chrome": "gulp pack --production --vendor=chrome",
    "build:firefox": "gulp pack --production --vendor=firefox",
    "build:opera": "gulp pack --production --vendor=opera",
    "build:edge": "gulp pack --production --vendor=edge",
    "dev": "yarn run dev:chrome",
    "dev:chrome": "gulp --watch --vendor=chrome",
    "dev:firefox": "gulp --watch --vendor=firefox",
    "dev:opera": "gulp --watch --vendor=opera",
    "dev:edge": "gulp --watch --vendor=edge",
    "lint": "standard"
  },
  "standard": {
    "globals": [
      "chrome"
    ]
  },
  "devDependencies": {
    "babel-cli": "6.x.x",
    "babel-core": "6.x.x",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "1.x.x",
    "chai": "4.x.x",
    "chromereload": "0.x.x",
    "debounce": "1.x.x",
    "del": "3.x.x",
    "directory-exists": "^2.0.0",
    "eslint": "^5.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-bump": "^3.1.1",
    "gulp-clean-css": "^3.9.4",
    "gulp-filter": "^5.1.0",
    "gulp-git": "^2.7.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.1.0",
    "gulp-json-transform": "^0.4.5",
    "gulp-less": "^4.0.1",
    "gulp-livereload": "^3.8.1",
    "gulp-plumber": "^1.2.0",
    "gulp-sass": "^4.0.1",
    "gulp-sequence": "^1.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tag-version": "^1.3.1",
    "gulp-zip": "^4.1.0",
    "oss-attribution-generator": "^1.7.1",
    "require-dir": "^1.0.0",
    "standard": "^11.0.1",
    "vinyl-named": "1.x.x",
    "webpack": "4.16.2",
    "webpack-stream": "5.0.0",
    "yargs": "^12.0.1"
  },
  "dependencies": {
    "diff-match-patch": "^1.0.0",
    "fast-levenshtein": "^2.0.6",
    "jquery": "^3.2.1",
    "md5-jkmyers": "^0.0.1",
    "underscore": "^1.9.1"
  }
}
